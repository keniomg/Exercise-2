using System.Collections;
using System;
using System.Text;
namespace lessons
{
    class program
    {
        static void Main()
        {
            int firstNumber = 5;
            int secondNumber = 12;
            int deltaNumber = secondNumber - firstNumber;
            int lastNumber = 96;
            int nextNumber = 0;
            Console.WriteLine(firstNumber);

            for (int i = 5;  i < lastNumber; i+=7)
            {
                nextNumber = firstNumber += deltaNumber;
                Console.WriteLine(nextNumber);
            }
        }
    }
}
