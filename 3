using System.Collections;
using System;
using System.Text;
namespace lessons
{
    class program
    {
        static void Main()
        {
            int num0 = 5;         
            int num1 = 12;
            int deltaNum = num1 - num0;
            int cycleLength = 13;
            Console.WriteLine(num0);

            for (int i = 0; i < cycleLength; i++)
            {
                num0 += deltaNum;
                Console.WriteLine(num0);
            }
        }
    }
}
