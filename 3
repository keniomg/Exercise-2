using System.Collections;
using System;
using System.Text;
namespace lessons
{
    class program
    {
        static void Main()
        {
            int firstSequenceNumber = 5;         
            int secondSequenceNumber = 12;
            int deltaNum = secondSequenceNumber - firstSequenceNumber;
            int lastSequenceNumber = 96;
            int nextSequenceNumber = 0;
            Console.WriteLine(firstSequenceNumber);

            while (nextSequenceNumber < lastSequenceNumber)
            {
                nextSequenceNumber = firstSequenceNumber += deltaNum;
                Console.WriteLine(nextSequenceNumber);
            }
        }
    }
}
